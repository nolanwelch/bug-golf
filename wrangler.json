  "env": {
    "dev": {
      "vars": {
        "ENVIRONMENT": "development",
        "CLERK_PUBLISHABLE_KEY": "pk_test_dG91Z2gtYnV6emFyZC04Ni5jbGVyay5hY2NvdW50cy5kZXYk"
      },
      "kv_namespaces": [
